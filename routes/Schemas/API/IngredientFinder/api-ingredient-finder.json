{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": [
        {
            "type": "object",
            "properties": {
                "store": {
                    "type": "string"
                },
                "data": {
                    "type": "array",
                    "items": [
                        {
                            "type": "object",
                            "properties": {
                                "ingredient_name": {
                                    "type": "string"
                                },
                                "product_id": {
                                    "type": "integer"
                                },
                                "product_name": {
                                    "type": "string"
                                },
                                "price": {
                                    "type": "string"
                                },
                                "amount": {
                                    "type": "string"
                                },
                                "units": {
                                    "type": "string"
                                }
                            },
                            "required": [
                                "ingredient_name",
                                "product_id",
                                "product_name",
                                "price",
                                "amount",
                                "units"
                            ]
                        }
                    ]
                }
            },
            "required": [
                "store",
                "data"
            ]
        }
    ]
}
